{
  "name": "gl-vectors",
  "version": "0.1.0",
  "description": "A javascript library that imitates glsl vectors",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node dist/index.js",
    "build": "rimraf dist/* && tsc --emitDeclarationOnly --declaration --outFile dist/index.d.ts && tsc -b",
    "build:watch": "npm run build -- --watch",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prenaissance/gl-vectors.git"
  },
  "author": "prenaissance",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prenaissance/gl-vectors/issues"
  },
  "homepage": "https://github.com/prenaissance/gl-vectors#readme",
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "jest": "^29.2.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
}